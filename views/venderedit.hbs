<div class="container">

    <form class="well form-horizontal" action="/venderedit/update" id="contact_form"
        style="background-color:#dddcdc ;margin-top: 40px;padding-top: 40px;padding-right: 100px;border-radius:5px;margin-right:20px;margin-bottom:20px;"
        name="" {{!-- onsubmit="return validation()" --}} method="post">
        <fieldset>

            <!-- Form Name -->
            <legend>
                <center>
                    <h1><b>Vender Form</b></h1>
                </center>
            </legend><br>
			<!-- Text input-->


            <!-- Text input-->
            <input class="form-control" id="id" name="id" type="hidden" aria-label="Fullname"
				value="{{data._id}}" aria-describedby="basic-addon1">


            <div class="form-group row row">
                <label class="col-md-5 control-label">
                    <h4 style="text-align: right;"><b>Fullname</b></h4>
                </label>
                <div class="col-md-7 inputGroupContainer">
                    <div class="input-group">
                        {{!-- <input name="first_name" placeholder="First Name" class="form-control" type="email">
                        --}}
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="basic-addon1">
                                <i class="fa fa-user fa-1x" style="background: white;"></i>
                            </span>
                        </div>
                        <input type="text" class="form-control" value="{{data.fullName}}" id="name" name="name"
                            aria-label="Fullname" aria-describedby="basic-addon1">
                        <span class="text-danger" name="cust_name_error" id="cust_name_error"></span>
                    </div>
                </div>
            </div>

            <!-- Text input-->

            <div class="form-group row row ">
                <label class="col-md-5 control-label">
                    <h4 style="text-align: right;"><b>Phone/Whatsup</b></h4>
                </label>
                <div class="col-md-7 inputGroupContainer">
                 
                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="basic-addon1">
                                <i class="fa fa-phone fa-1x" style="background: white;"></i>
                            </span>
                        </div>
                        <input type="text" name="mobile" id="mobile" class="form-control" placeholder="Phone"
                            aria-label="Username" aria-describedby="basic-addon1"
                            value="{{data.mobile}}">
                        <span class="text-danger" name="phone_error" id="phone_error"></span>
                    </div>
                </div>
            </div>

            <!-- Text input-->
            <div class="form-group row">
                <label class="col-md-5 control-label">
                    <h4 style="text-align: right;"><b>E-Mail@</b></h4>
                </label>
                <div class="col-md-7 inputGroupContainer">
                    <div class="input-group">
                        {{!-- <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span> --}}
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="basic-addon1">
                                <i class="fa fa-envelope fa-1x" style="background: white;"></i>
                            </span>
                        </div>
                        <input name="email" id="email" placeholder="Customer email" class="form-control" type="email"
                        value="{{data.email}}">
                    </div>
                </div>
            </div>

            <div class="form-group row">
                <label class="col-md-5 control-label">
                    <h4 style="text-align: right;"><b>City</b></h4>
                </label>
                <div class="col-md-7 selectContainer">
                    <div class="input-group">
                        {{!-- <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span> --}}
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="basic-addon1">
                                <i class="fa fa-plane fa-1x" style="background: white;"></i>
                            </span>
                        </div>
                      
                        <input name="city" id="city" class="form-control" type="text" placeholder="City"
                        value="{{data.city}}">
                        <span class="text-danger" name="city" id="city"></span>
                    </div>
                </div>
            </div>

            <div class="form-group row">
                <label class="col-md-5 control-label">
                    <h4 style="text-align: right;"><b>State</b></h4>
                </label>
                <div class="col-md-7 selectContainer">
                    <div class="input-group">
                        {{!-- <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span> --}}
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="basic-addon1">
                                <i class="fa fa-plane fa-1x" style="background: white;"></i>
                            </span>
                        </div>
                       
                        <input name="state" id="state" class="form-control" type="text" placeholder="State"
                        value="{{data.state}}">
                        <span class="text-danger" name="city_error" id="city_error"></span>
                    </div>
                </div>
            </div>

            <div class="form-group row">
                <label class="col-md-5 control-label">
                    <h4 style="text-align: right;"><b>GST No.</b></h4>
                </label>
                <div class="col-md-7 selectContainer">
                    <div class="input-group">
                        {{!-- <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span> --}}
                        <div class="input-group-prepend">
